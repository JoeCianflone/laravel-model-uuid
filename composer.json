{
    "name": "dyrynda/laravel-model-uuid",
    "description": "This package allows you to easily work with UUIDs in your Laravel models.",
    "license": "MIT",
    "keywords": [
        "eloquent",
        "laravel",
        "model",
        "uuid"
    ],
    "authors": [
        {
            "name": "Michael Dyrynda",
            "email": "michael@dyrynda.com.au",
            "homepage": "https://dyrynda.com.au"
        }
    ],
    "require": {
        "php": ">=5.6.4 || >= 7.0",
        "illuminate/database": "5.3.* || 5.4.* || 5.5.* || 5.6.*",
        "illuminate/events": "5.3.* || 5.4.* || 5.5.* || 5.6",
        "ramsey/uuid": "~3.6"
    },
    "autoload": {
        "psr-4": {
            "Dyrynda\\Database\\Support\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "suggest": {
        "dyrynda/laravel-efficient-uuid": "Override Laravel's default query grammar to efficiently store UUIDs. (~1.0)"
    },
    "minimum-stability": "stable"
}
